<div>
  <h2>Webradio</h2><br/>
  <span id="infoSong"></span>
  <audio src="http://{{ip}}:{{port}}/stream.mp3" id="player" autoplay></audio>
  <div>
        <button onclick="document.getElementById('player').volume+=0.1">Volume +</button>
        <button onclick="document.getElementById('player').volume-=0.1">Volume -</button>
        <button onclick="document.getElementById('player').muted = document.getElementById('player').muted ? false : true">Muter</button>
  </div>
</div>
